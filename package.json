{
  "name": "quiz_multiplayer_game",
  "version": "1.0.0",
  "description": "multiplayer quiz game",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest --coverage",
    "dev": "set NODE_ENV=development&& nodemon src/server/server.js",
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/server.js"
  },
  "author": "Glenn Kenneth Linden",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "supertest": "^3.1.0",
    "jest": "^23.4.2",
    "jsdom": "^11.12.0",
    "nodemon": "^1.18.4",
    "react-addons-test-utils": "^15.6.2",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-dev-middleware": "latest",
    "webpack-hot-middleware": "latest"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "tests/.*\\.test\\.(js|jsx)$",
    "collectCoverageFrom": [
      "src/**.(js|jsx)"
    ]
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "latest",
    "cookie-parser": "^1.4.3",
    "cookie-sessions": "0.0.2",
    "express": "latest",
    "express-session": "^1.15.6",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport.socketio": "latest",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "socket.io": "2.1.1",
    "socket.io-client": "2.1.1"
  }
